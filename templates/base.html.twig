<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">

    <title>
        {% block title %}
        Welcome!
        {% endblock %}
    </title>

    {% block stylesheets %}

  


    {{ encore_entry_link_tags('app') }}

    <link crossorigin="anonymous" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css"
        integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" rel="stylesheet">
    <link rel="icon" href="{{asset('/images/favicon.jpeg')}}">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no user-scalable=no">
         {# <link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'> #}
    {% endblock %}
    {% block head %}

        <style>
        .superdiv {
            overflow-x: hidden !important;
            /* position : relative; */

        }


  
        .btn-info {
            background-color: #808B96;
        }

        .btn-info:hover {
            background-color: #ABB2B9;
 
        }
        </style>



            {% endblock %}

</head>

<body>
<div class="superdiv">

    {% include('navbar.html.twig') %}
    {# this handle all the alert in this web #}
    {% for label, messages in app.flashes %}
    {% for message in messages %}
    <div class="container alert alert-{{ label }}">
        {{ message }}
    </div>
    {% endfor %}
    {% endfor %}
        {% block body %}
        {% endblock %}
    

    {{include('footer.html.twig')}}
    {% block javascript %}
    {{ encore_entry_script_tags('app') }}

    {% endblock %}


</div>
</body>

</html>